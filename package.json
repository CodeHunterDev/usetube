{
  "name": "usetube",
  "version": "1.3.1",
  "description": "crawl youtube without api key (search videos, search channels, get all channel/playlist's videos)",
  "main": "dist/usetube.min.js",
  "directories": {
    "test": "test"
  },
  "dependencies": {
    "axios": "^0.21.0",
    "moment": "^2.28.0",
    "node-fetch": "^2.6.1",
    "typescript": "^4.0.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.1",
    "@babel/core": "^7.12.3",
    "@types/node": "^14.14.7",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1"
  },
  "scripts": {
    "test": "node test/back.js",
    "build": "npx tsc --build && npx minify dist/usetube.js > dist/usetube.min.js && cp dist/usetube.min.js test/usetube.min.js",
    "dev": "npx nodemon -e ts --exec 'yarn build && yarn test'",
    "push": "npm publish"
